#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules/jsmin/bin/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules/jsmin/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules/jsmin/bin/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules/jsmin/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/jsmin@1.0.1/node_modules:/Users/admin/Downloads/Proyectos Programacion/Taller1/moviereviewsproject/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsmin@1.0.1/node_modules/jsmin/bin/jsmin" "$@"
else
  exec node  "$basedir/../../../../../jsmin@1.0.1/node_modules/jsmin/bin/jsmin" "$@"
fi
